{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "comp-automations-inngest",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-20",
  "compatibility_flags": ["nodejs_compat"],
  "triggers": {
    "crons": ["0 */6 * * *"]
  },
  "r2_buckets": [
    {
      "binding": "MY_BUCKET",
      "bucket_name": "bucket-competition",
      "remote": true
    }
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "keep_vars": true
}
